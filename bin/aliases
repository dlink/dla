# dla alias file

export d="$HOME/dla" ; alias d='cd $d'
export db="$d/bin"   ; alias db='cd $db'
export dc="$d/conf"  ; alias dc='cd $dc'
export dl="$d/lib"   ; alias dl='cd $dl'
export ds="$d/sql"   ; alias ds='cd $ds'
export dw="$d/web"   ; alias dw='cd $dw'
export dwi="$dw/images"; alias dwi='cd $dwi'

# database
alias ddb='psql dla'
